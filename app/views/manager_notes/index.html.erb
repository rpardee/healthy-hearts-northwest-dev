<p id="notice"><%= notice %></p>

<h1>Listing Manager Notes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manager_notes.each do |manager_note| %>
      <tr>
        <td><%= link_to 'Show', manager_note %></td>
        <td><%= link_to 'Edit', edit_manager_note_path(manager_note) %></td>
        <td><%= link_to 'Destroy', manager_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manager note', new_manager_note_path %>
