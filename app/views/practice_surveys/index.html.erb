<p id="notice"><%= notice %></p>

<h1>Listing Practice Surveys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practice_surveys.each do |practice_survey| %>
      <tr>
        <td><%= link_to 'Show', practice_survey %></td>
        <td><%= link_to 'Edit', edit_practice_survey_path(practice_survey) %></td>
        <td><%= link_to 'Destroy', practice_survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Practice survey', new_practice_survey_path %>
