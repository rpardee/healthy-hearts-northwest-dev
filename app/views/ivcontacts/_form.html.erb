<%= form_for(@ivcontact) do |f| %>
  <% if @ivcontact.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@ivcontact.errors.count, "error") %> prohibited this IV Contact from being saved:</h2>

      <ul>
      <% @ivcontact.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

