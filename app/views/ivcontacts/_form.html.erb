      <td><%= f.label :contact_dt, "Date of Contact" %></td>
      <td><%= f.text_field :contact_dt, value: @ivcontact.contact_dt.try(:strftime, "%m/%d/%Y"), class: 'datepicker' %> mm/dd/yyyy</td>
