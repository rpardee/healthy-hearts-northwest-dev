<div id='wrapper'>
<%= render 'layouts/header' %>
<p id="notice"><%= notice %></p>

<h1>Coach Assignments by Practice</h1>

<table class='typical enable-data-table'>
  <thead>
    <tr>
      <th>Practice</th>
      <th>Coach</th>
    </tr>
  </thead>

  <tbody>
    <% @practices.each do |practice| %>
      <tr>
        <td><%= link_to practice.name, edit_coach_practice_path(practice) %></td>
        <td>
          <% if practice.coach %>
            <%= link_to practice.coach.name, list_coach_practice_path(practice.coach) %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />
<%= link_to "Back", :back %>
</div>