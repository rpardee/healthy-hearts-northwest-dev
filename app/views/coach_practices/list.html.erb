<div id='wrapper'>
<%= render 'layouts/header' %>
<p id="notice"><%= notice %></p>

<h1>Coach View</h1>

<h2> Limit this to the current coach </h2>

<table class='typical enable-data-table'>
  <thead>
    <tr>
      <th>Practice</th>
      <th>Coach</th>
      <th>New Contact</th>
    </tr>
  </thead>

  <tbody>
    <% @practices.each do |practice| %>
      <tr>
        <td><%= link_to practice.name, edit_coach_practice_path(practice) %></td>
        <td><%= practice.coach.name if practice.coach %></td>
        <td><%= link_to image_tag("plus-blue.png", size: "20x20", alt: "Add New Contact"), new_coach_practice_event_path(practice), class: 'addButton', title: 'Add New Contact', rel: 'tooltip' %>
      </tr>
    <% end %>
  </tbody>
</table>

</div>