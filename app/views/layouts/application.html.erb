<!DOCTYPE html>
<html>
<head>
  <title>Healthy Hearts Northwest</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'apple-touch-icon-57x57', rel: 'apple-touch-icon', sizes: '57x57' %>
  <%= favicon_link_tag 'apple-touch-icon-60x60', rel: 'apple-touch-icon', sizes: '60x60' %>
  <%= favicon_link_tag 'apple-touch-icon-72x72', rel: 'apple-touch-icon', sizes: '72x72' %>
  <%= favicon_link_tag 'apple-touch-icon-76x76', rel: 'apple-touch-icon', sizes: '76x76' %>
  <%= favicon_link_tag 'apple-touch-icon-114x114', rel: 'apple-touch-icon', sizes: '114x114' %>
  <%= favicon_link_tag 'apple-touch-icon-120x120', rel: 'apple-touch-icon', sizes: '120x120' %>
  <%= favicon_link_tag 'apple-touch-icon-144x144', rel: 'apple-touch-icon', sizes: '144x144' %>
  <%= favicon_link_tag 'apple-touch-icon-152x152', rel: 'apple-touch-icon', sizes: '152x152' %>
  <%= favicon_link_tag 'apple-touch-icon-180x180', rel: 'apple-touch-icon', sizes: '180x180' %>
  <%= favicon_link_tag 'favicon-32x32.png', rel: 'icon', sizes: '32x32', type: 'image/png' %>
  <%= favicon_link_tag 'android-chrome-192x192.png', rel: 'icon', sizes: '192x192', type: 'image/png' %>
  <%= favicon_link_tag 'favicon-96x96.png', rel: 'icon', sizes: '96x96', type: 'image/png' %>
  <%= favicon_link_tag 'favicon-16x16.png', rel: 'icon', sizes: '16x16', type: 'image/png' %>
  <%= favicon_link_tag 'manifest.json', rel: 'manifest' %>
  <meta name="theme-color" content="#ffffff">
</head>
<body>
	<p id="notice"><%= notice %></p>
  <p id="alert"><%= alert %></p>

	<%= yield %>

</body>
</html>
