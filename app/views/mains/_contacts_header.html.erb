<thead>
  <tr>
    <th><%= @boxlab || "Required Contacts" %></th>
    <% (0..14).each do |i| %>
      <%
        extrlab = case i
          when 0
            '<br/>(welcome)'
          when 3, 6
            '<br/>(visit)'
          end
      %>
      <th class='contact-date-column'>#<%= raw("#{i+1} #{extrlab}") %> </th>
    <% end %>
  </tr>
  <tr>
    <th>Date</th>
      <% (0..14).each do |i| %>
        <% this_contact = @ivcs[i] %>
        <% if this_contact then %>
          <th><%= link_to this_contact.contact_dt, coach_practice_path(this_contact.practice) %></th>
        <% else %>
          <th>&nbsp;</th>
        <% end %>
      <% end %>
  </tr>
</thead>