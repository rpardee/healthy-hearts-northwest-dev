<div id='wrapper'>
<%= render 'layouts/header' %>
<p id="notice"><%= notice %></p>

<h1>Listing All Partners</h1>

<table class='typical'>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partners.each do |partner| %>
      <tr>
        <td><%= link_to partner.name, partner %></td>
        <td><%= link_to 'Edit', edit_partner_path(partner) %></td>
        <!-- td><%= link_to 'Change Password', edit_partner_registration_path(partner) %></td -->
        <td><%= link_to 'Destroy', partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add New Partner', partners_admin_new_url %>
</div>